[display]

window/size/width=640
window/size/height=480
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/godot_transitions/plugin.cfg", "res://addons/texture_fonts/plugin.cfg" )

