}

[application]

config/name="IntegratedBox"
run/main_scene="res://Levels/Level1.tscn"
config/icon="res://icon.png"

[autoload]

Transitions="*res://addons/godot_transitions/transitions.gd"
globals="*res://Scripts/Global.gd"
